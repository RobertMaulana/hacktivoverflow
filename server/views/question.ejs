<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>List</title>
  </head>
  <body>
    <table>
      <thead>
        <tr>
          <th>Title</th>
          <th>Content</th>
          <th>Action</th>
        </tr>
      </thead>
      <tbody>
        <% data.forEach((questions) =>{ %>
          <tr>
            <td><%= questions.title_question %></td>
            <td><%= questions.question %></td>
            <td><a href="/question/<%= questions.id %>"><%= questions.id %></a></td>
          </tr>
        <%})%>
      </tbody>
    </table>
  </body>
</html>
